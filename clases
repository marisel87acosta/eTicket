class Curso(object):
 	def __init__(self, n= None):
 		self.nombre=n
 		self.alumnos= list()
 	def agregarAlumno(self, alumno):
 		self.alumnos.append(alumno)
 	del mostrarMis alumnos(self):
 		for alumno in self.alumnos:
 			print('alumnos: ', alumno.nombre, alumno.apellido)
 			
